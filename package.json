{
  "name": "high-ui",
  "version": "0.0.0",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs-version:build": "cross-env BUIDL_DOC_VERSION=version npm run docs:build",
    "build": "father-build && npm run build:clean",
    "build:clean": "node scripts/cleanUmdTypeFile.js",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint-fix:js && npm run lint-fix:style",
    "lint:js": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint-fix:js": "npm run lint:js -- --fix",
    "lint:style": "stylelint src/**/*.less",
    "lint-fix:style": "npm run lint:style -- --fix",
    "commit": "cz",
    "precommit": "lint-staged"
  },
  "main": "lib/index.js",
  "module": "es/index.esm.js",
  "typings": "es/index.d.ts",
  "gitHooks": {
    "commit-msg": "commitlint --edit \"$1\""
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ],
    "**/*.{js,jsx,ts,tsx}": "eslint"
  },
  "author": "caijf <369756941@qq.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "classnames": "^2.3.1",
    "doly-icons": "^1.1.1",
    "rc-hooks": "^1.8.2",
    "react-transition-group": "^4.4.2",
    "swiper": "^7.4.1"
  },
  "peerDependencies": {
    "react": ">=16.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@types/color": "^3.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.178",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@umijs/test": "^3.0.5",
    "antd": "^4.17.1",
    "babel-plugin-import": "^1.13.3",
    "chalk": "^4.1.2",
    "color": "^4.0.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^1.0.17",
    "eslint": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "father-build": "^1.17.2",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "prettier-plugin-two-style-order": "^1.0.1",
    "react": "^16.12.0",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^16.12.0",
    "stylelint": "^14.1.0",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "yorkie": "^2.0.0"
  },
  "resolutions": {
    "react": "^16.12.0",
    "@types/react": "^16.12.0",
    "@types/history": "4"
  }
}